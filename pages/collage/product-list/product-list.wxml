<!--pages/collage/product-list/product-list.wxml-->
<view class="jn-main">
  <view class="content-list">
    <view class="collage-list">
      <view class="clearfix">
        <view class="left-img fl">
          <image src="{{item.imgUrl}}"></image>
        </view>
        <view class="right-cont fl">
          <view class="list-name">
            <text>拼多多</text>
            <text class="btn-collage">去开团</text>
          </view>
          <view class="price-items">
            <view class="price-details absolute">
              <text class="">拼团价</text>
              <text class="present-price">100.00</text>
              <text>元</text>
              <text class="original-price">66.66元</text>
            </view>
            <view class="collage-details absolute">
              <text>3人团·</text>
              <text class="had-collage">已成500团</text>
            </view>
          </view>
        </view>
      </view>
    </view>
  </view> 
</view>